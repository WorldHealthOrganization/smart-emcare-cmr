    "name": "valueset-appetite-test",
    "title": "EmCare.B22.DE23",
    "status": "draft",
    "experimental": false,
    "description": "Appetite Test (using RUTF) Results",
    "immutable": true,
    "compose": {
        "include": [
            {
                "system": "https://fhir.dk.swisstph-mis.ch/matchbox/fhir/CodeSystem/emcare-custom-codes",
                "concept": [
                    {
                        "code": "EmCare.B22.DE24",
                        "display": "Able to finish RUTF",
                        "designation": [
                            {
                                "value": "The client was able to finish the RUTF (the client was able to finish the minimum amount of RTUF (at least one-third of a packet of RUTF portion (92g) or 3 teaspoons from a pot within 30 minutes)"
                            }
                        ]
                    },
                    {
                        "code": "EmCare.B22.DE25",
                        "display": "Unable to finish RUTF",
                        "designation": [
                            {
                                "value": "The client was unable to finish the RUTF (the client was unable to eat one-third of a packet of RUTF portion (92g) or 3 teaspoons from a pot within 30 minutes)"
                            }
                        ]
                    },
                    {
                        "code": "EmCare.B22.DE26",
                        "display": "RUTF Not Available",
                        "designation": [
                            {
                                "value": "The healthcare worker did not have any RUTF available to carry out the appetite test"
                            }
                        ]
                    }
                ]
            }
        ]
    }
}